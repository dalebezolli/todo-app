<mvc:View controllerName="com.bezolli.primary.controller.Home"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m">
    <Page id="page" title="{i18n>title}">
        <content>
            <List id="list" items="{path: '/TodoList'}">
                <items>
                    <StandardListItem title="{name}" />
                </items>
            </List>
        </content>
    </Page>
</mvc:View>
